{
    "name": "dict.cc context menu",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Add a context-menu for dict.cc",
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": ["contextMenu.js"],
        "persistent": false
    },
    "icons": {
        "16": "static/favicon.ico"
    },
    "default_locale": "en"
}
